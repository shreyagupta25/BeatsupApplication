package apps;

public class Amstrong {

	
  public static boolean chkAms(int num)
	{
		int num1=num;
		int sum=0;
		int r=0;
		boolean flag=false;
		while(num>0)
		{
			r=num%10;
			sum=sum+(r*r*r);
			num=num%10;
		}
		if(num1==sum)
			flag=true;
		
		return flag;
		
	}
	public static void main(String[] args)
	{
		System.out.println("Shreya");
		System.out.println(chkAms(10));
		System.out.println("Shreya");
		
	}

}
